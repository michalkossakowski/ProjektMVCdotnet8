@model IEnumerable<ProjektMVCdotnet8.Entities.PostEntity>
@{
    ViewData["Title"] = "CategoryIndex";
}

@foreach(var element in Model)
{
    <div class="card mb-3">
        <div class="card-body">
            <img src="/attachments/@element.AttachmentSource" alt="Alternate Text" />
            <h5 class="card-title">@element.Title</h5>
            <p class="card-text">@element.PostContent</p>
            <p class="card-text"><small class="text-body-secondary">@element.CreatedDate</small></p>
        </div>
    </div>
}

